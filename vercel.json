{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.ts",
        "use": "@vercel/node"
      },
      {
        "src": "api/**/*.tsx",
        "use": "@vercel/node"
      },
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "api/**/*.jsx",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1",
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
          "Access-Control-Allow-Headers": "Content-Type, Authorization"
        }
      }
    ],
    "functions": {
      "api/**/*.ts": {
        "maxDuration": 20
      },
      "api/**/*.tsx": {
        "maxDuration": 20
      },
      "api/**/*.js": {
        "maxDuration": 20
      },
      "api/**/*.jsx": {
        "maxDuration": 20
      }
    }
  }
  